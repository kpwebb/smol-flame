{
  "scripts": {
    "make": "bun run --hot src/server.tsx | bun run dev:css",
    "make:css": "tailwindcss  -o static/main.css --watch",
    "bake" :"bun run bake.ts && bun run build:css",  
    "bake:css": "tailwindcss  -o dist/static/main.css --minify",
    "shake" :"deploy"  
  },
  "dependencies": {
    "hono": "^3.11.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.7",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.6"
  }
}
